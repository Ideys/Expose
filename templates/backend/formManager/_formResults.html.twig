{% spaceless %}
{% for result in results %}
  {{ result.date|date('d/m/Y H:i') }}
  <table>
    <thead>
      <tr>
        {% for attr_name, data in result.result %}
          <th>{{ attr_name }}</th>
        {% endfor %}
      </tr>
    </thead>
    <tbody>
      <tr>
        {% for data in result.result %}
          <td>
          {% if data is iterable %}
            {{ data|join(' ; ') }}
          {% else %}
              {{ data }}
          {% endif %}
          </td>
        {% endfor %}
      </tr>
    </tbody>
  </table>
{% else %}
  {{ 'form.no.results'|trans }}
{% endfor %}
{% endspaceless %}