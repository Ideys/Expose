{% spaceless %}
<ul class="{{ class }}">
  {% if not settings.maintenance or is_granted('ROLE_ADMIN') %}
    {% include "frontend/menu/_sideLinks.html.twig" with {
        'widgets': widgets,
        'class': class,
        'menuPos': 'main',
        'follow': follow
    } only %}
  {% endif %}
</ul>
<ul class="secondary {{ class }}">
  {% include "frontend/menu/_sideLinks.html.twig" with {
        'widgets': widgets,
        'class': class,
        'menuPos': 'second',
        'follow': follow
  } only %}
  {% include "frontend/menu/_adminLinks.html.twig" only %}
  {% if 'disabled' != settings.contactSection %}
  <li>
    <a href="{{ path('contact') }}"{{ follow }}>
      {{ 'contact.contact'|trans }}
    </a>
  </li>
  {% endif %}
</ul>
{% endspaceless %}